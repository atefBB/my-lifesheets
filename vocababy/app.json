{
	"app": {
		"name": "Vocababy",
		"slug": "vocababy",
		"entry_name": "word set",
		"entries_name": "word sets",
		"description": "learning to speak",
		"private": false,
		"icon": "üë∂üèº",
		"color": "#7d4988",
		"storage_type": "",
		"storage": "",
		"custom_css": "",
		"charts_placement": ""
	},
	"temporal": {
		"times": "",
		"ranges": ""
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"prefix": "[child_name & \" was \"]",
				"expression": "duration(date - birthday, \"months\")",
				"suffix": " old"
			},
			"name": "child_age_readable"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "count(word)",
				"suffix": " new words",
				"formatting": [
					"b"
				]
			},
			"name": "new_words"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "$previous.total_words + new_words",
				"suffix": " total words",
				"prefix": "/ "
			},
			"name": "total_words"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"allow_multiple": "yes",
				"initial_items": "0",
				"appearance": "line",
				"type": "text"
			},
			"name": "word"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "property",
			"scope": "root",
			"settings": {
				"type": "text",
				"default": "Zoe",
				"suffix": ""
			},
			"name": "child_name"
		},
		{
			"type": "property",
			"scope": "root",
			"settings": {
				"prefix": "was born on ",
				"type": "date",
				"default": "2019-07-12"
			},
			"name": "birthday"
		},
		{
			"type": "spacer",
			"scope": "root",
			"settings": {}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "‚ûï Add word set",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "stretch",
				"set_pairs": [],
				"filter_pairs": []
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "sum",
				"aggregate_of": "new_words",
				"caption": "New words per month",
				"type": "bar",
				"orientation": "horizontal"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "sum",
				"aggregate_of": "total_words",
				"caption": "Total words per month",
				"type": "bar",
				"orientation": "horizontal"
			}
		}
	]
}