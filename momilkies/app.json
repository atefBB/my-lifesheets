{
	"app": {
		"name": "Mo‚ÄôMilkies",
		"slug": "momilkies",
		"entry_name": "nursing session",
		"entries_name": "nursing sessions",
		"description": "nursing session tracker",
		"private": false,
		"icon": "ü§±üèΩ",
		"color": "#c591ba",
		"storage_type": "",
		"storage": "",
		"custom_css": "",
		"charts_placement": ""
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes"
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"prefix": "(",
				"suffix": ")",
				"expression": "duration(duration, 2)"
			},
			"name": "duration_readable"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "options",
				"options": "Left, Right"
			},
			"name": "side"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [
					{
						"name": "side",
						"value": "\"Left\""
					}
				],
				"filter_pairs": [],
				"label": "Start from Left"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [
					{
						"name": "side",
						"value": "Right"
					}
				],
				"filter_pairs": [],
				"label": "Start from Right"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"target": "last(entry)",
				"target_preset": "last",
				"appearance": "",
				"set_pairs": [
					{
						"name": "side",
						"value": "if(last(entry).side = \"Left\", \"Right\", \"Left\")"
					}
				],
				"filter_pairs": [],
				"label": "Switch breast"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"target": "last(entry)",
				"target_preset": "last",
				"appearance": "",
				"set_pairs": [
					{
						"name": "end_time",
						"value": "time($now)"
					}
				],
				"filter_pairs": [],
				"label": "Unlatched"
			}
		}
	],
	"charts": []
}