{
	"app": {
		"name": "MyBod",
		"slug": "mybody",
		"entry_name": "measurement",
		"description": "measurement tracker",
		"private": false,
		"icon": "üí™üèº",
		"color": "#5fa6c4",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "measurements"
	},
	"temporal": {
		"times": "yes",
		"ranges": ""
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {
				"appearance": "line"
			}
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"suffix": "kg",
				"default": "",
				"prefix": "Weight: "
			},
			"name": "weight"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"suffix": "cm",
				"default": "",
				"prefix": "Chest: "
			},
			"name": "bust"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"suffix": "cm",
				"default": "",
				"prefix": "Waist: "
			},
			"name": "waist"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"suffix": "cm",
				"default": "",
				"prefix": "Hip: "
			},
			"name": "hip"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"prefix": "BMI: ",
				"expression": "weight / (height/100 * height/100)"
			},
			"name": "bmi"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "property",
			"scope": "root",
			"settings": {
				"prefix": "Height: ",
				"default": "160",
				"suffix": "cm"
			},
			"name": "height"
		},
		{
			"type": "spacer",
			"scope": "root",
			"settings": {
				"appearance": "line"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "Add entry",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "stretch",
				"set_pairs": [],
				"filter_pairs": []
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "line",
				"aggregate": "average",
				"aggregate_of": "bmi",
				"group_by": "day",
				"smooth": true
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "line",
				"aggregate": "average",
				"aggregate_of": "weight",
				"group_by": "day",
				"smooth": true
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "line",
				"aggregate": "average",
				"aggregate_of": "bust",
				"group_by": "day",
				"caption": "Bust per day",
				"smooth": true
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "line",
				"aggregate": "average",
				"aggregate_of": "waist",
				"group_by": "day",
				"caption": "Waist circumference per day",
				"smooth": true
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "line",
				"aggregate": "average",
				"aggregate_of": "hip",
				"group_by": "day",
				"caption": "Hip per day",
				"smooth": true
			}
		}
	]
}