{
	"app": {
		"name": "mygraine",
		"slug": "mygraine",
		"entryName": "migraine",
		"icon": "ðŸ¤•",
		"color": "#cc0044",
		"storageType": "",
		"storage": "https://github.com/leaverou/my-lifesheets/mygraine/data.json",
		"notes": true,
		"description": "migraine tracker",
		"entries_name": "migraines",
		"entry_name": "migraine"
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes",
		"defaultDate": "[$today]",
		"defaultTime": "",
		"showDuration": true
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"expression": "duration(duration, 2)",
				"prefix": "(",
				"suffix": ")"
			},
			"name": "duration_readable"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "number",
				"min": 1,
				"max": 5,
				"step": 1,
				"number_style": "bar",
				"icon": "ðŸ¤•",
				"default": "",
				"bar_style": "discrete"
			},
			"name": "intensity"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "options",
				"options": "left, right, both"
			},
			"name": "side"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "duration(end_date_time - $previous.end_date_time, 2)",
				"visible": "[end_time and $previous.end_time]",
				"suffix": " since last migraine"
			}
		}
	],
	"root": [
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "count(entry)",
				"suffix": " recorded migraines â€¢"
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "duration($now - last(end_date_time), 2)",
				"suffix": " since last migraine",
				"visible": "[last(end_time)]"
			}
		},
		{
			"type": "spacer",
			"scope": "root",
			"settings": {
				"appearance": "line"
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"target_preset": "new",
				"target": "add(entry)",
				"appearance": "stretch",
				"label": "Start migraine",
				"set_pairs": [],
				"filter_pairs": []
			}
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"target_preset": "last",
				"target": "last(entry)",
				"label": "End migraine",
				"appearance": "stretch",
				"set_pairs": [
					{
						"name": "end_time",
						"value": "time($now)"
					}
				],
				"filter_pairs": []
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "count",
				"aggregate_of": "entry",
				"orientation": "horizontal",
				"type": "bar"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate_of": "side",
				"group_by": "side"
			}
		}
	]
}